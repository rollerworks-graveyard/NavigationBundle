{
    "name": "rollerworks/navigation-bundle",
    "description": "Symfony bundle for defining menu-structures and breadcrumbs in the KnpMenuBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastiaan Stok",
            "email": "s.stok@rollerscapes.net"
        }
    ],
    "require": {
        "php": ">=5.3",
        "symfony/framework-bundle": "~2.3",
        "symfony/expression-language": "~2.4",
        "knplabs/knp-menu-bundle": "^2.0.0"
    },
    "require-dev": {
        "matthiasnoback/symfony-config-test": "~1.1.0",
        "matthiasnoback/symfony-dependency-injection-test": "~0.7.4",
        "symfony/phpunit-bridge": "~2.7"
    },
    "autoload": {
        "psr-4": {
            "Rollerworks\\Bundle\\NavigationBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rollerworks\\Bundle\\NavigationBundle\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.2-dev"
        }
    }
}
